<% if (site.data.projects) { %>
    <% if (page.source !== "projects/index.md") { %>
      <section id="projects">
      <h1><a href="<%- url_for("projects") %>"><%= __('index.projects') %></a></h1>
    <% } else { %>
      <h1><a href="#projects" class="headerlink"><%= __('index.projects') %></a></h1>
    <% } %>
      <ul class="project-list">
        <% for(var obj in site.data.projects){ %>
          <% if (page.source !== "projects/index.md") { %>
            <li class="project-item">
              <a href="<%= site.data.projects[obj].url %>"><%= site.data.projects[obj].name %></a>: <%- markdown(site.data.projects[obj].desc) %>
            </li>
          <% } else { %>
            <%- partial('_partial/project/item', { item: site.data.projects[obj], key: obj }) %>
          <% } %>
        <% } %>
      </ul>
    <% if (page.source !== "projects/index.md") { %>
      </section>
    <% } %>
<% } %>